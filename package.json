{
  "name": "monorepo",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "packageManager": "npm@11.6.2",
  "type": "module",
  "devDependencies": {
    "globby": "^16.1.0",
    "picomatch": "^4.0.3",
    "prettier": "^3.7.4",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "turbo": "^2.7.3"
  },
  "workspaces": [
    "packages/**",
    "apps/**",
    "cdk/**"
  ],
  "scripts": {
    "// -- üöÄ Turborepo": "--",
    "build": "turbo run build",
    "build:no-cache": "turbo run build --force",
    "clean": "node scripts/clean.mjs",
    "sequence": "node scripts/run-sequence.mjs",
    "cache:clear": "rm -rf .turbo",
    "// -- üö¢ Deploy Orchestrator": "--",
    "deploy": "node scripts/deploy-orchestrator.mjs",
    "deploy:status": "node scripts/deploy-orchestrator.mjs status",
    "deploy:prepare": "node scripts/deploy-orchestrator.mjs prepare",
    "deploy:infra": "node scripts/deploy-orchestrator.mjs deploy infra",
    "deploy:lambdas": "node scripts/deploy-orchestrator.mjs deploy lambdas",
    "deploy:all": "node scripts/deploy-orchestrator.mjs deploy all",
    "prelint": "npm run format:markdown",
    "lint": "turbo run lint",
    "prelint:fix": "npm run format:markdown",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "// -- ‚èèÔ∏è Ejection": "--",
    "eject:backend-only": "node scripts/eject-aspect.mjs backend-only",
    "eject:analytics": "node scripts/eject-aspect.mjs analytics",
    "eject:users": "node scripts/eject-aspect.mjs users",
    "// -- ü§ñ Agent Scripts": "--",
    "format:markdown": "prettier --write \".claude/**/*.md\"",
    "test:scripts": "node --test \"scripts/utils/__tests__/*.mjs\"",
    "phase:check": "npm run lint:fix && node .claude/scripts/check-code-quality.mjs && npm run build && npm run test",
    "agent:finalize": "npm run phase:check && npm run build:no-cache"
  }
}
