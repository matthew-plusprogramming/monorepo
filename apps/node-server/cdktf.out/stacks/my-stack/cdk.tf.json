{
  "//": {
    "metadata": {
      "backend": "s3",
      "stackName": "my-stack",
      "version": "0.21.0"
    },
    "outputs": {
      "my-stack": {
        "applicationLogGroupName": "applicationLogGroupName",
        "serverLogStreamName": "serverLogStreamName",
        "userTableName": "userTableName",
        "verificationTableName": "verificationTableName"
      }
    }
  },
  "output": {
    "applicationLogGroupName": {
      "description": "The name of the application log group",
      "value": "${aws_cloudwatch_log_group.application-logs.name}"
    },
    "serverLogStreamName": {
      "description": "The name of the server log stream",
      "value": "${aws_cloudwatch_log_stream.server-logs.name}"
    },
    "userTableName": {
      "description": "The name of the user table",
      "value": "${aws_dynamodb_table.user-table.name}"
    },
    "verificationTableName": {
      "description": "The name of the verification table",
      "value": "${aws_dynamodb_table.verification-table.name}"
    }
  },
  "provider": {
    "aws": [
      {
        "region": "us-west-1"
      }
    ]
  },
  "resource": {
    "aws_cloudwatch_log_group": {
      "application-logs": {
        "//": {
          "metadata": {
            "path": "my-stack/application-logs",
            "uniqueId": "application-logs"
          }
        },
        "name": "application-logs"
      }
    },
    "aws_cloudwatch_log_stream": {
      "server-logs": {
        "//": {
          "metadata": {
            "path": "my-stack/server-logs",
            "uniqueId": "server-logs"
          }
        },
        "log_group_name": "${aws_cloudwatch_log_group.application-logs.name}",
        "name": "server-logs"
      }
    },
    "aws_dynamodb_table": {
      "user-table": {
        "//": {
          "metadata": {
            "path": "my-stack/user-table",
            "uniqueId": "user-table"
          }
        },
        "attribute": [
          {
            "name": "id",
            "type": "S"
          },
          {
            "name": "email",
            "type": "S"
          }
        ],
        "billing_mode": "PAY_PER_REQUEST",
        "global_secondary_index": [
          {
            "hash_key": "email",
            "name": "email-index",
            "projection_type": "ALL"
          }
        ],
        "hash_key": "id",
        "name": "my-example-bucket-user-table",
        "region": "us-west-1"
      },
      "verification-table": {
        "//": {
          "metadata": {
            "path": "my-stack/verification-table",
            "uniqueId": "verification-table"
          }
        },
        "attribute": [
          {
            "name": "id",
            "type": "S"
          }
        ],
        "billing_mode": "PAY_PER_REQUEST",
        "hash_key": "id",
        "name": "my-example-bucket-verification-table",
        "region": "us-west-1",
        "ttl": {
          "attribute_name": "ttl",
          "enabled": true
        }
      }
    }
  },
  "terraform": {
    "backend": {
      "s3": {
        "bucket": "mycompany-myproject-cdktf-state-00001",
        "dynamodb_table": "mycompany-myproject-cdktf-lock-00001",
        "encrypt": true,
        "key": "stack/my-stack/terraform.tfstate",
        "region": "us-west-1"
      }
    },
    "required_providers": {
      "aws": {
        "source": "aws",
        "version": "6.9.0"
      }
    }
  }
}