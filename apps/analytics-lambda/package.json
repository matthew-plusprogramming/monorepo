{
  "name": "analytics-lambda",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "// -- üöÄ Development": "--",
    "// TODO: Consolidate into one script that also makes sure cdk outputs are there": "",
    "dev": "npm run copy-cdk-outputs && mkdir dist || true && dotenvx run -f .env.dev -- concurrently 'vite build --mode dev --clearScreen false --watch' 'cd dist && node --watch index.cjs'",
    "preview": "dotenvx run -f .env.production -- node --watch dist/index.cjs",
    "// -- üõ†Ô∏è Build & Clean": "--",
    "prebuild": "tsc",
    "build": "dotenvx run -f .env.production -- vite build --mode production",
    "postbuild": "npm run copy-cdk-outputs && npm run copy-env-prod",
    "copy-cdk-outputs": "dotenvx run -f .env.production -- cross-env ENV=production tsx scripts/copy-cdk-outputs.ts",
    "copy-env-dev": "cross-env ENV=development tsx scripts/copy-env.ts",
    "copy-env-prod": "cross-env ENV=production tsx scripts/copy-env.ts",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "// -- üß™ Testing": "--",
    "pretest": "tsc",
    "test": "echo 'No tests configured yet'",
    "test:coverage": "npm run test -- --coverage",
    "// -- üßπ Linting & Formatting": "--",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --fix --cache",
    "lint:no-cache": "eslint src",
    "// -- ‚úÖ Verification & CI": "--",
    "// -- üîß Miscellaneous": "--",
    "encrypt-envs": "dotenvx encrypt -f .env.dev .env.production",
    "decrypt-envs": "dotenvx decrypt -f .env.dev .env.production&& echo 'WARNING: DO NOT COMMIT decrypted .env files to the repository!'"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.926.0",
    "@aws-sdk/util-dynamodb": "^3.926.0",
    "@cdk/backend-server-cdk": "*",
    "@dotenvx/dotenvx": "^1.51.1",
    "@packages/backend-core": "*",
    "@packages/schemas": "*",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@configs/eslint-config": "*",
    "@configs/ts-config": "*",
    "@configs/vite-config": "*",
    "@configs/vitest-config": "*",
    "@types/aws-lambda": "^8.10.157",
    "@types/supertest": "^6.0.3",
    "@utils/ts-utils": "*",
    "@vitest/coverage-v8": "^4.0.7",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.7"
  }
}
