{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contract Registry",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "type", "path", "owner", "version"],
    "properties": {
      "id": { "type": "string" },
      "type": { "type": "string" },
      "path": { "type": "string" },
      "owner": { "type": "string" },
      "version": { "type": "integer", "minimum": 1 }
    },
    "additionalProperties": false
  }
}
