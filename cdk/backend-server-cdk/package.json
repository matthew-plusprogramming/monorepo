{
  "name": "@cdk/backend-server-cdk",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": "./src/consumer/index.ts"
  },
  "scripts": {
    "// -- üöÄ Development": "--",
    "// -- üõ†Ô∏è Build & Clean": "--",
    "copy-assets-for-cdk": "tsx scripts/copy-lambda-artifacts.ts",
    "// TODO: ": "Make the lambda go into a subfolder of dist",
    "postcopy-assets-for-cdk": "cd dist && zip -r ./lambda.zip . && cd ..",
    "clean": "rm -rf node_modules && rm -rf cdktf.out && rm -rf .turbo && rm -rf dist && rm -rf cdktf-outputs",
    "// -- üß™ Testing": "--",
    "// -- üßπ Linting & Formatting": "--",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --fix --cache",
    "lint:no-cache": "eslint src",
    "// -- ‚úÖ Verification & CI": "--",
    "cdk:bootstrap:migrate": "tsx scripts/cdk-bootstrap-migrate.ts",
    "// TODO: this should copy lambda artifacts in a pre script": "",
    "cdk:deploy": "cross-env-shell 'dotenvx run -f .env.$ENV -- cdktf deploy'",
    "cdk:destroy": "cross-env-shell 'dotenvx run -f .env.$ENV -- cdktf destroy'",
    "cdk:synth": "cross-env-shell 'dotenvx run -f .env.$ENV -- cdktf synth'",
    "cdk:output": "cross-env-shell 'dotenvx run -f .env.$ENV -- cdktf output --outputs-file cdktf-outputs/stacks/$STACK/outputs.json $STACK'",
    "cdk:bootstrap:migrate:dev": "cross-env ENV=dev npm run cdk:bootstrap:migrate",
    "cdk:bootstrap:migrate:prod": "cross-env ENV=production npm run cdk:bootstrap:migrate",
    "cdk:deploy:dev": "cross-env ENV=dev npm run cdk:deploy",
    "cdk:deploy:prod": "cross-env ENV=production npm run cdk:deploy",
    "cdk:destroy:dev": "cross-env ENV=dev npm run cdk:destroy",
    "cdk:destroy:prod": "cross-env ENV=production npm run cdk:destroy",
    "cdk:synth:dev": "cross-env ENV=dev npm run cdk:synth",
    "cdk:synth:prod": "cross-env ENV=production npm run cdk:synth",
    "cdk:output:dev": "cross-env ENV=dev tsx scripts/cdk-output.ts",
    "cdk:output:prod": "cross-env ENV=production tsx scripts/cdk-output.ts",
    "// -- üîß Miscellaneous": "--",
    "encrypt-envs": "dotenvx encrypt -f .env.dev .env.production",
    "decrypt-envs": "dotenvx decrypt -f .env.dev .env.production&& echo 'WARNING: DO NOT COMMIT decrypted .env files to the repository!'"
  },
  "devDependencies": {
    "@cdktf/provider-aws": "^21.14.0",
    "@configs/eslint-config": "*",
    "@configs/ts-config": "*",
    "@dotenvx/dotenvx": "^1.51.0",
    "@packages/schemas": "*",
    "@utils/type-utils": "*",
    "cdktf": "^0.21.0",
    "constructs": "^10.4.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  }
}
