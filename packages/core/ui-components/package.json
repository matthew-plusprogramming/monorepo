{
  "name": "@ui/components",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "types": "./src/index.ts",
  "scripts": {
    "dev": "npm run gen:css-types -- --watch",
    "prebuild": "npm run gen:css-types",
    "build": "tsc -p tsconfig.json",
    "gen:css-types": "typed-scss-modules src --aliasPrefixes.@ src --exportType default --outputFolder __generated__"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "framer-motion": "^12.23.26"
  },
  "peerDependencies": {
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@configs/ts-config": "*",
    "typed-scss-modules": "^8.1.1",
    "typescript": "^5.9.3"
  },
  "sideEffects": [
    "./src/**/*.scss"
  ]
}
